{
  "compilerOptions": {
    "noEmit": true,
    "types": ["node"],
    "esModuleInterop": true,
    "module": "NodeNext",
    "target": "ES2022",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedSideEffectImports": true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts"]
}
